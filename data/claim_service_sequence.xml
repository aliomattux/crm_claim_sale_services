<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
	<!-- Return -->
        <record id="seq_type_crm_claim_service_return" model="ir.sequence.type">
            <field name="name">CRM Claim Return</field>
            <field name="code">crm.claim.return</field>
        </record>

        <record id="seq_crm_claim_service_return" model="ir.sequence">
            <field name="name">Claim Return</field>
            <field name="code">crm.claim.return</field>
            <field name="prefix">RMA/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

	<!-- Refund Only -->
        <record id="seq_type_crm_claim_service_refund" model="ir.sequence.type">
            <field name="name">CRM Claim Refund</field>
            <field name="code">crm.claim.refund</field>
        </record>

        <record id="seq_crm_claim_service_refund" model="ir.sequence">
            <field name="name">CRM Claim Refund</field>
            <field name="code">crm.claim.refund</field>
            <field name="prefix">RFND/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

	<!-- Exchange -->
        <record id="seq_type_crm_claim_service_exchange" model="ir.sequence.type">
            <field name="name">CRM Claim Exchange</field>
            <field name="code">crm.claim.exchange</field>
        </record>

        <record id="seq_crm_claim_service_exchange" model="ir.sequence">
            <field name="name">CRM Claim Exchange</field>
            <field name="code">crm.claim.exchange</field>
            <field name="prefix">RMAEX/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
